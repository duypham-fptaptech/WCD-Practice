<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>CreatePhone</servlet-name>
        <servlet-class>com.example.wcdpractice.controller.CreatePhone</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreatePhone</servlet-name>
        <url-pattern>/add-phone</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListPhone</servlet-name>
        <servlet-class>com.example.wcdpractice.controller.ListPhone</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListPhone</servlet-name>
        <url-pattern>/list-phone</url-pattern>
    </servlet-mapping>
</web-app>